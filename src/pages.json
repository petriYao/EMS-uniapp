{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^u-(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "XWAHeader": "@/components/Header/index.vue"
    }
  },

  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置接口的效果展示"
    },
    "scope.saveFile": {
      "desc": "你的文件将用于小程序文件保存"
    }
  },
  "pages": [
    {
      "path": "pages/login/login",
      "style": {
        "navigationBarTitleText": "登录",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/my/my",
      "style": {
        "navigationBarTitleText": "我的",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/product/product",
      "style": {
        "navigationBarTitleText": "产品",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/productionorder/productionorder",
      "style": {
        "navigationBarTitleText": "生产工单",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom",
        "navigationBarTextStyle": "black"
      }
    },
    {
      "path": "pages/productionreporting/productionreporting",
      "style": {
        "navigationBarTitleText": "生产报工",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom",
        "navigationBarTextStyle": "black"
      }
    },
    {
      "path": "pages/processlist/processlist",
      "style": {
        "navigationBarTitleText": "工序列表",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom",
        "navigationBarTextStyle": "white"
      }
    },
    {
      "path": "pages/saleOrder/OutboundOrder",
      "style": {
        "navigationBarTitleText": "出库单详情"
      }
    },
    {
      "path": "pages/storage/storage",
      "style": {
        "navigationBarTitleText": "入库",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/lowerCamelCase/PascalCase",
      "style": {
        "navigationBarTitleText": "销售出库",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/cancelOutbound/Index",
      "style": {
        "navigationBarTitleText": "销售出库撤销",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/barCodeStock/Index",
      "style": {
        "navigationBarTitleText": "条码库存查询",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/transferOrder/Index",
      "style": {
        "navigationBarTitleText": "调拨",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/purchaseStockIn/Index",
      "style": {
        "navigationBarTitleText": "采购入库",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/returnMaterial/purchaseReturn/Index",
      "style": {
        "navigationBarTitleText": "采购退货",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/returnMaterial/salesReturn/Index",
      "style": {
        "navigationBarTitleText": "销售退货",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/returnMaterial/otherOutbound/Index",
      "style": {
        "navigationBarTitleText": "其他出库",
        "navigationStyle": "custom"
      }
    }
  ],
  "tabBar": {
    "color": "#606266",
    "selectedColor": "#f7403f",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "static/index/home.png",
        "selectedIconPath": "static/index/homefill.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/product/product",
        "iconPath": "static/index/home.png",
        "selectedIconPath": "static/index/myfill.png",
        "text": "产品"
      },
      {
        "pagePath": "pages/my/my",
        "iconPath": "static/index/my.png",
        "selectedIconPath": "static/index/myfill.png",
        "text": "我的"
      }
    ]
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "uniIdRouter": {}
}
